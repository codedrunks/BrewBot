{
  "name": "brewbot",
  "version": "0.2.0",
  "scripts": {
    "start": "tsc && tsc-alias && node --enable-source-maps out/bot.js",
    "test": "ts-node src/test.ts",
    "lint": "tsc --noEmit && eslint .",
    "watch": "nodemon --watch .env --watch \"src/**/*\" -e ts --inspect npm start",
    "clearCommands": "ts-node src/tools/clearCommands.ts",
    "predeploy": "npx prisma generate && npx prisma migrate deploy",
    "deploy": "tsc && tsc-alias && node out/bot.js",
    "win-redis": "call src\\tools\\redis.bat",
    "link": "npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jabrils-Discord-Server/bot.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Jabrils-Discord-Server/bot/issues"
  },
  "homepage": "https://github.com/Jabrils-Discord-Server/bot#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.23",
    "@types/steamapi": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.18",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.11",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@prisma/client": "^3.15.1",
    "@types/yargs": "^17.0.11",
    "axios": "^0.27.2",
    "better-erela.js-spotify": "^1.3.8",
    "canvas": "^2.9.3",
    "discord-api-types": "^0.34.0",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.0",
    "erela.js": "^2.3.3",
    "fast-memoize": "^2.5.2",
    "fs-extra": "^10.1.0",
    "fuse.js": "^6.6.2",
    "kleur": "^4.1.4",
    "prisma": "^3.15.1",
    "redis": "^4.2.0",
    "simple-statistics": "^7.7.5",
    "steamapi": "^2.2.0",
    "svcorelib": "^1.16.0",
    "yargs": "^17.5.1"
  },
  "bin": {
    "brewbot": "./src/tools/cli.js"
  },
  "nodemonConfig": {
    "watch": [
      "src/*"
    ],
    "ignore": [
      "out/*",
      "node_modules/*",
      "test.js",
      "test.ts"
    ]
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}